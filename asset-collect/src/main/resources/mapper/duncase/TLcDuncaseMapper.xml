    <select id="selectDuncaseByTime" resultMap="mapResult">
        select id, case_no, custom_name, certificate_no,  custom_phone, month_repay_day,first_overdue_time, overdue_days, overdue_aging,max_overdue_day,org_id,repay_date,borrow_line,
        borrow_card_no,borrow_bank,total_interest_rmb,total_principal_rmb,total_default_interest_rmb,total_expenses_payable_rmb,lowest_payment_rmb,total_debt_amount_rmb,
        appoint_case_balance,create_time,create_by,modify_time,modify_by,transfer_type,enter_coll_date,close_case_yhje,overdue_fine,city,area,recommend_vendor,recommend_website,
        product_name,repay_method,aging_periods,bill_address,year_interest_rate,day_interest_rate,first_overdue_sign,total_overdue_day,overdue_frequency,import_batch_no,pack_no,
        back_case_date,loan_type,stay_case_flag,risk_flag,contract_type,reduction_flag,legal_flag,ljyhje from t_lc_duncase
        where 1 = 1
        <if test="createTime != null">
            AND (create_time <![CDATA[>=]]> #{createTime} AND create_time <![CDATA[<]]> #{modifyTime}) OR (modify_time <![CDATA[>=]]> #{createTime} AND modify_time <![CDATA[<]]> #{modifyTime})
        </if>
        order by id desc
        limit #{pnum},#{psize}
    </select>

    <select id="selectDuncaseCount" resultType="java.lang.Integer">
        select count(1) from t_lc_duncase
        where 1 = 1
        <if test="createTime != null">
            AND (create_time <![CDATA[>=]]> #{createTime} AND create_time <![CDATA[<]]> #{modifyTime}) OR (modify_time <![CDATA[>=]]> #{createTime} AND modify_time <![CDATA[<]]> #{modifyTime})
        </if>
    </select>

    <select id="searchAllDuncaseTotalMoney" resultType="java.util.Map">
        SELECT
        sum(t.arrears_total) as totalArrears,
        sum(t.dqyhje) as totalPay
        FROM t_lc_duncase d, t_lc_task t, sys_user su WHERE t.owner_id = su.user_id and  d.case_no = t.case_no AND d.import_batch_no = t.import_batch_no
        <if test="orgId != null  and orgId != ''">
            and d.org_id = #{orgId}
        </if>
        and (1=1
        <if test="caseNo != null  and caseNo != ''">
            and d.case_no = #{caseNo}
        </if>
        <if test="userGroup != null and userGroup != ''">
            and su.user_group = #{userGroup}
        </if>
        <if test="customName != null  and customName != ''">
            and d.custom_name like concat('%', #{customName}, '%')
        </if>
        <if test="taskStatus != null ">
            and t.task_status = #{taskStatus}
        </if>
        <if test="callCodeHistoryList != null and callCodeHistoryList.size()>0">
            and EXISTS(SELECT r.call_sign FROM t_lc_call_record r WHERE r.case_no = d.case_no and r.call_sign IN
            <foreach collection="callCodeHistoryList" item="cl" open="(" close=")" separator=",">
                #{cl}
            </foreach>
            )
        </if>
        <if test="freeSQL != null  and freeSQL != ''">
            ${freeSQL}
        </if>
        )
    </select>


    <update id="updateScore" parameterType="java.util.List">
